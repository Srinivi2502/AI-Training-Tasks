      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "from sklearn.naive_bayes import BernoulliNB\n"
      ],
      "metadata": {
        "id": "dobdK3VMySER"
      },
      "execution_count": 12,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "texts = [\n",
        "    \"I love this product\",\n",
        "    \"This is amazing and fantastic\",\n",
        "    \"I am extremely satisfied\",\n",
        "\n",
        "    \"I hate this product\",\n",
        "    \"Worst experience ever\",\n",
        "    \"Not happy at all\"\n",
        "]\n",
        "\n",
        "labels = [1,1,1, 0,0,0]  # 1 = Positive, 0 = Negative\n",
        "\n",
        "data = pd.DataFrame({\"Text\": texts, \"Sentiment\": labels})"
      ],
      "metadata": {
        "id": "PfrksRlyyofG"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "vectorizer = CountVectorizer(binary=True)\n",
        "X = vectorizer.fit_transform(data[\"Text\"])\n",
        "y = np.array(labels)"
